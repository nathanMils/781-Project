{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
      "default": {
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
      }
    },
    "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "formatter": "default",
        "level": "DEBUG"
      }
    },
    "loggers": {
      "util": {
        "level": "INFO",
        "handlers": ["console"],
        "propagate": true
      },
      "util.random": {
        "level": "INFO",
        "handlers": ["console"],
        "propagate": false
      },
      "util.data": {
        "level": "INFO",
        "handlers": ["console"],
        "propagate": false
      }
    }
  }
  